	  <item name="android:textColor">#000000</item>
  </style>

  <style name="CheckBoxMapAppTheme" parent="android:Widget.CompoundButton.CheckBox">
      <item name="android:button">@drawable/mapapptheme_btn_check_holo_light</item>
  </style>

  <style name="RadioButtonMapAppTheme" parent="android:Widget.CompoundButton.RadioButton">
      <item name="android:button">@drawable/mapapptheme_btn_radio_holo_light</item>
  </style>

  <style name="ButtonMapAppTheme" parent="android:Widget.Button">
	  <item name="android:background">@drawable/mapapptheme_btn_default_holo_light</item>
	  <item name="android:minHeight">48dip</item>
	  <item name="android:minWidth">64dip</item>
	  <item name="android:textColor">#000000</item>
  </style>

  <style name="ImageButtonMapAppTheme" parent="android:Widget.ImageButton">
	  <item name="android:background">@drawable/mapapptheme_btn_default_holo_light</item>
  </style>

  <style name="SpinnerMapAppTheme" parent="android:Widget.Spinner">
      <item name="android:background">@drawable/mapapptheme_spinner_background_holo_light</item>
